


      <div class="right_col" role="main">
        <div class="">

          <div class="page-title">
            <div class="title_left">
              <h3>Add New Unit Trust</h3>
            </div>
            <div class="title_right">
              <div class="col-md-5 col-sm-5 col-xs-12 form-group pull-right top_search">
                <div class="input-group">
                  <input type="text" class="form-control" placeholder="Search for...">
                  <span class="input-group-btn">
                            <button class="btn btn-default" type="button">Go!</button>
                        </span>
                </div>
              </div>
            </div>
          </div>
          <div class="clearfix"></div>
          <div class="row">
            <div class="col-md-12 col-sm-12 col-xs-12">
              <div class="x_panel">
                <div class="x_title">
                  <h2>NOTE: <small>NAME MUST BE EXACTLY AS STATED ON THE WEBSITE</small></h2>
                  <ul class="nav navbar-right panel_toolbox">
                    <li><a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
                    </li>
                    <li><a class="close-link"><i class="fa fa-close"></i></a>
                    </li>
                  </ul>
                  <div class="clearfix"></div>
                </div>
                <div class="x_content">
                  <br />
					         <%= form_for @unit_trust, html: {class: "form-horizontal form-label-left"} do |f|%>
                    <div class="form-group">


                      <label class="control-label col-md-3 col-sm-3 col-xs-12" for="first-name">Unit Trust Name
                      </label>
                      <div class="col-md-6 col-sm-6 col-xs-12">
                      	<%= f.text_field :name, class:"form-control col-md-7 col-xs-12"%>
                      </div>
                    </div>
                    <div class="form-group">
                      <label class="control-label col-md-3 col-sm-3 col-xs-12" for="first-name">Starting Month
                      </label>
                      <div class="col-md-6 col-sm-6 col-xs-12">
                      	<%= f.date_field :start_date, class:"form-control col-md-7 col-xs-12"%>
                      </div>
                    </div>
                    <div class="form-group">                      
                      <label class="control-label col-md-3 col-sm-3 col-xs-12" for="first-name">Price of Unit
                      </label>
                      <div class="col-md-6 col-sm-6 col-xs-12">
                      	<%= f.number_field :original_price, :step => 'any', class:"form-control col-md-7 col-xs-12"%>
                      </div>
                    </div>
                    <div class="form-group">                      
                      <label class="control-label col-md-3 col-sm-3 col-xs-12" for="first-name">Number of Units
                      </label>
                      <div class="col-md-6 col-sm-6 col-xs-12">
                      	<%= f.number_field :original_num_units, :step => 'any', class:"form-control col-md-7 col-xs-12"%>
                      </div>
                    </div>
                    <div class="form-group">                      
                      <label class="control-label col-md-3 col-sm-3 col-xs-12" for="first-name">URL link
                      </label>
                      <div class="col-md-6 col-sm-6 col-xs-12">
                      	<%= f.text_field :url, class:"form-control col-md-7 col-xs-12"%>
                      </div>
                    </div>
                    <div class="ln_solid"></div>
                    <div class="form-group">
                      <div class="col-md-6 col-sm-6 col-xs-12 col-md-offset-3">
              			<%= f.submit "Submit", class: "btn btn-primary"%>
                      </div>
                    </div>
                    <%end%>
                </div>
              </div>
            </div>
          </div>







        </div>
      </div>